# Navigate to your project folder
cd path/to/api_integration_app

# Initialize a git repository if it's not initialized yet
git init

# Add your remote GitHub repository URL
git remote add origin https://github.com/your_username/api_integration_app.git

# Add files to the repository
git add .

# Commit the files
git commit -m "Initial commit with API integration app"

# Push the files to GitHub
git push -u origin master
